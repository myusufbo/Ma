<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yusufboss.myapplication" >
    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HealthCalculator"
            android:label="@string/title_activity_health_calculator" >
        </activity>
        <activity
            android:name=".MotherHealth"
            android:label="@string/title_activity_mother_health_info">
        </activity>
        <activity
            android:name=".ChildHealth"
            android:label="@string/title_activity_child_health_info">
        </activity>
        <activity
            android:name=".Hospital"
            android:label="@string/title_activity_hospital" >
        </activity>
        <activity
            android:name=".EmergencyContact"
            android:label="@string/title_activity_emergency_contact" >
        </activity>
        <activity
            android:name=".Vaccination"
            android:label="@string/title_activity_vaccination" >
        </activity>
    </application>

</manifest>
